{
	"name": "Fate Extra",
	"author": "Met",
	"target": "System-Wide",
	"version": "v1.0.23",
	"description": "A Fate Extra inspired theme in celebration of Fate Extra Record coming later this year.",
	"manifest_version": 5,
	"inject": {
		"shared.css": [
			"SP",
			"MainMenu",
			"QuickAccess"
		]
	},
	"dependencies": {
		"No Home Filter": {
			"Opacity Control": "1"
		},
		"DellyFooter": {
			"Effect": "Gradient",
			"Size": "Default",
			"Rounded corners": "No",
			"Normal font": "No"
		},
		"Top Bar Transparency": {
			"Transparency": ".0",
			"Always Add Transparency": "Yes"
		},
		"Game Header Text Stroke": {}
	},
	"patches": {
		"Colorize": {
			"type": "checkbox",
			"default": "Yes",
			"values": {
				"Yes": {
					"colorchange.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				},
				"No": {}
			},
			"components": []
		},
		"Font Options": {
			"type": "dropdown",
			"default": "Italic Font (Most faithful)",
			"values": {
				"Italic Font (Most faithful)": {
					"italicfontchange.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				},
				"Normal Font (Less faithful)": {
					"fontchange.css": [
						"SP",
						"MainMenu",
						"QuickAccess"
					]
				},
				"No change to font": {}
			},
			"components": []
		}
	}
}